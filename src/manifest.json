{
  "manifest_version": 3,
  "background": {
    "service_worker": "background.ts"
  },
  "content_scripts": [
    {
      "js": [
        "content/index.tsx"
      ],
      "matches": [
        "https://*.upwork.com/*",
        "https://chat.openai.com/*"
      ]
    }
  ],
  "permissions": [
    "webRequest",
    "storage",
    "notifications"
  ],
  "host_permissions": [
    "*://*.upwork.com/*",
    "*://chat.openai.com/*"
  ]
}